---
layout: null
---

{
  "posts": [
    {% for page in site.pages %}{
      "title": "{{ page.title }}",
      "url": "{{ page.url | prepend: site.baseurl }}",
      "date": "{{ page.date | date: "%B %-d, %Y" }}",
      "raw_date": "{{ page.date }}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}